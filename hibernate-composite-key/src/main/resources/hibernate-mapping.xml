<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entities.Book">
        <composite-id>
            <key-property name="bookId"/>
            <key-property name="isbn"/>
        </composite-id>
        <property name="name"/>
        <property name="price"/>
    </class>
</hibernate-mapping>